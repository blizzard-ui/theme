{
  "printWidth": 100,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "quoteProps": "consistent",
  "overrides": [
    {
      "files": ["*.json"],
      "options": {
        "useTabs": false
      }
    },
    {
      "files": ["*.ts", "*.tsx", "*.js"],
      "options": {
        "useTabs": true
      }
    }
  ]
}
